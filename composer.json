{
    "name": "appsdept/lankabell-sms",
    "description": "A simple package to connect LankaBell SMS Api, with 2factor Authentication",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "LasanthaSudeera",
            "email": "lasan.sudeera@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "7.4.x-dev"
    },

    "autoload": {
        "psr-4": {
            "AppsDept\\LaravelLankaBellSMS\\": "src/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "AppsDept\\LaravelLankaBellSMS\\LankaBellSMS"
            ],
            "aliases": {
                "LankaBellSMS": "AppsDept\\LaravelLankaBellSMS\\Facades\\LankaBellSMS"
            }
        }
    }
}
